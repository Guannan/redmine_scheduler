<% form_for :level, @level, :url => { :action => "create" }, :html => {:class => "nifty_form"} do |f| %>
  <%= f.select :user_id, @users.map {|u| [u.name, u.id] } %>
  <%= select_tag (:skill_id, options_for_select(@skills_list, @skill_choice)) %>
  <%= f.select :rating, 1..3, :disabled => [2,3] %>
  <%= submit_tag "Create" %>
<% end %>
