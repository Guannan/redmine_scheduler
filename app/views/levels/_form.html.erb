<% form_for :level, @level, :url => { :action => "create" }, :html => {:class => "nifty_form"} do |f| %>
  <%= f.select :user_id, @users.map {|u| [u.name, u.id] } %>
  <%= f.select :skill_id, @skills.map {|s| [ s.name, s.id ] } %>
  <%= f.select :rating, 1..3 %>
  <%= submit_tag "Create" %>
<% end %>
