
<% form_for :level, @level, :url => { :action => "create" }, :html => {:class => "nifty_form"} do |f| %>
  <%= f.select :user_id, @users.map {|u| [u.name, u.id] } %>
  <br><br/>

  <% @skills.each do |skill| %>
    <% if !skill.name.nil? %>
      <p><%= check_box_tag 'skill_id[]',skill.id %>
      <%= skill.name %><p/>
    <%end%>
  <% end %>


  <%= submit_tag "Create" %>
<% end %>
